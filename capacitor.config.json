{
  "appId": "com.invepin.barmanagement",
  "appName": "Invepin",
  "webDir": "dist",
  "ios": {
    "scheme": "Invepin",
    "contentInset": "automatic",
    "allowsLinkPreview": false,
    "scrollEnabled": true,
    "backgroundColor": "#0F172A",
    "preferredContentMode": "mobile"
  },
  "plugins": {
    "Camera": {
      "permissions": [
        "camera"
      ]
    },
    "StatusBar": {
      "style": "dark",
      "backgroundColor": "#0F172A"
    },
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#0F172A",
      "showSpinner": true,
      "spinnerColor": "#3B82F6"
    }
  },
  "bundledWebRuntime": false
}